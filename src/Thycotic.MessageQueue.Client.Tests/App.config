        <bindingRedirect oldVersion="0.0.0.0-3.5.0.0" newVersion="3.5.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Thycotic.Discovery.Core" publicKeyToken="33ff8c00c367b219" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.0.0.7" newVersion="1.0.0.7" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Thycotic.Discovery.Sources" publicKeyToken="0ee4a5cc35cfd219" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.0.0.0" newVersion="1.0.0.0" />
